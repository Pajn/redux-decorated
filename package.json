{
  "name": "redux-decorated",
  "version": "0.2.0-dev.1",
  "description": "Helpers for Redux designed to work great with Typescript",
  "main": "./index.js",
  "scripts": {
    "postinstall": "npm-run-all postinstall:*",
    "postinstall:tsdm": "tsdm rewire",
    "postinstall:mkdir": "mkdir test/node_modules",
    "postinstall:clean": "rm -f test/node_modules/redux-decorated",
    "postinstall:link-tests": "ln -s ../.. test/node_modules/redux-decorated",
    "prepublish": "babel --out-dir . src",
    "watch": "babel --out-dir . src --watch",
    "test": "cd test && tsc && mocha --recursive bin/specs",
    "test-watch": "cd test && (tsc --watch & mocha --harmony -r babel-polyfill --watch --recursive bin/specs)"
  },
  "author": "Rasmus Eneman <rasmus@eneman.eu>",
  "license": "Apache-2.0",
  "devDependencies": {
    "babel-cli": "^6.3.15",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-stage-2": "^6.3.13",
    "chai": "^3.5.0",
    "mocha": "^2.4.5",
    "mock-functions": "^0.1.1",
    "retyped-chai-tsd-ambient": "^3.4.0-0",
    "retyped-mocha-tsd-ambient": "^2.2.5-0",
    "retyped-redux-tsd-ambient": "^1.0.0-0",
    "tsdm": "0.1.0-3",
    "tslint": "^3.3.0",
    "typescript": "^1.7.5"
  },
  "peerDependencies": {
    "redux": "3.x"
  },
  "directories": {
    "lib": "./lib"
  },
  "bugs": {
    "url": "https://github.com/Pajn/redux-decorated/issues"
  },
  "repository": "Pajn/redux-decorated",
  "typescript": {
    "definition": "typings.d.ts"
  }
}
